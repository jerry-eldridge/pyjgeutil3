from mapto import MapTo
from math import pi,cos,sin,sqrt

def meters(inch):
    return inch*0.0254
def inch_to_ft(inch):
    return inch/12.0
def Celcius(fahrenheit):
    F = fahrenheit
    C = MapTo(32,0,212,100,F)
    return C

def Kelvin(celcius):
    C = celcius
    K = C + 273.15
    return K

def Fahrenheit(celcius):
    C = celcius
    F = MapTo(0,32,100,212,C)
    return F
def mph_to_mps(mph):
    return mph*0.44704
def mps_to_mph(mps):
    return mps/0.44704
def meters_to_ft(meters):
    return 3.28084*meters
def inches_to_meters(inches):
    return inches*0.0254
def ft_to_meters(feet):
    return 0.3048*feet
def meters_to_yards(meters):
    return meters_to_ft(meters)/3.0
def deg_to_rad(deg):
    return deg*pi/180.0
def rad_to_deg(rad):
    return rad*180.0/pi
def newtons_to_lbs(newtons): #pounds of force given force in newtons
    return newtons*0.224808943
def lbs_to_kg(lbs):
    return lbs*0.453592 #kg
def rpm_to_radps(rpm):
    return rpm*0.1047 #1 rotation per minute as radians per second
def meters_to_miles(meters):
     return meters/1609.34   
